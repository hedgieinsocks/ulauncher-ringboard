    "triggers": {
        "regular": {
            "keyword": "c",
            "name": "Literal Search",
            "description": "Look for exact matches in the clipboard history"
        },
        "fuzzy": {
            "keyword": "cf",
            "name": "Fuzzy Search",
            "description": "Look for fuzzy matches in the clipboard history"
        }
    }
}
